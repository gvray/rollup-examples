{
  "name": "rollup-watch-example",
  "version": "1.0.0",
  "description": "Rollup watch mode example",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev:livereload": "rollup -c rollup.livereload.config.js -w",
    "dev:hmr": "rollup -c rollup.hmr.config.js -w",
    "serve": "serve dist"
  },
  "keywords": [
    "rollup",
    "watch",
    "livereload",
    "hmr",
    "hot-module-replacement"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup": "^2.75.6",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "date-fns": "^2.28.0"
  }
}